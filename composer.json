{
    "name": "codetetic/pest-plugin-prophecy",
    "description": "Pest Prophecy Plugin",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Measham",
            "email": "github@codetetic.co.uk"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "pestphp/pest": "^2.36",
        "phpspec/prophecy": "^1.22",
        "phpspec/prophecy-phpunit": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Pest\\Prophecy\\": "src/"
        },
        "files": [
            "src/Autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^6.4",
        "phpstan/phpstan": "^2.1",
        "jangregor/phpstan-prophecy": "^2.1",
        "phpstan/extension-installer": "^1.4",
        "rector/rector": "^2.0"
    }
}
